# Uncomment the next line to define a global platform for your project

platform :ios, '13.2'
use_frameworks!
workspace 'KKdayPortal.xcworkspace'

def shared
  # UI
  pod 'SnapKit', '~> 5.0.1'

  # Firebase 
  pod 'Firebase/Core', '~> 6.13.0'
  pod 'Fabric', '~> 1.10.2'
  pod 'Crashlytics', '~> 3.14.0'

  # Google
  pod 'GoogleSignIn', '~> 5.0.2'

  # Rx
  pod 'RxCocoa', '~> 4.5.0'
  pod 'Moya/RxSwift', '~> 13.0.1'
  pod 'RxDataSources', '~> 3.1.0'
  pod 'RxKeyboard', '~> 0.9.0'
  pod 'RxGesture', '~> 2.2.0'
end

target 'KKdayPortal-Sit' do
  project 'KKdayPortal.xcodeproj'
  shared

  target 'KKdayPortal-SitTests' do
    inherit! :search_paths
  end
end

target 'KKdayPortal-Stage' do
  project 'KKdayPortal.xcodeproj'
  shared

end

target 'KKdayPortal-Test' do
  project 'KKdayPortal.xcodeproj'
  shared

end

target 'KKdayPortal-Production' do
  project 'KKdayPortal.xcodeproj'
  shared

end



